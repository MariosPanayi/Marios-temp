simple <- emmeans(anova, ~Day*probability)
simple_Stg1_Dur <- apa_print(contrast(simple, interaction = "pairwise", by = "Day", adjust = "tukey"))
anova_Stg1_Dur$table
simple_Stg1_Dur$table
